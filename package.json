    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "mdsvex": "^0.12.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.72.2"
  },
  "type": "module"
}